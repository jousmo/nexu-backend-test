{
  "name": "borrar",
  "version": "2.0.0",
  "description": "Nexu Backend Test",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "dev": "nodemon .",
    "lint": "eslint .",
    "test": "npm run db:clean:test && NODE_ENV=test ava",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "db:build:database": "dotenv cross-var node ./lib/sequelize/buildModels.js",
    "db:seed:generate": "sequelize-cli seed:generate --name",
    "db:seed:load": "dotenv cross-var sequelize-cli db:seed:all",
    "db:seed:undo": "dotenv cross-var sequelize-cli db:seed:undo --seed",
    "db:seed:undo:all": "dotenv cross-var sequelize-cli db:seed:undo:all",
    "db:init": "npm run db:build:database && npm run db:seed:load",
    "db:clean:test": "NODE_ENV=test dotenv cross-var sequelize-cli db:drop && NODE_ENV=test dotenv cross-var sequelize-cli db:create && NODE_ENV=test npm run db:build:database && NODE_ENV=test npm run db:seed:load"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "ava": "^4.3.1",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.20.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "nodemon": "^2.0.19",
    "sequelize-cli": "^6.4.1",
    "standard": "^17.0.0",
    "standard-version": "^9.5.0",
    "supertest": "^6.2.4"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "helmet": "^5.1.1",
    "joi": "^17.6.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.3"
  }
}
